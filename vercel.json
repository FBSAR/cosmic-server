{
  "version": 2,
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/players", "dest": "/api/players.ts" },

    { "src": "/api/auth/six-digit-code-login", "dest": "/api/auth/six-digit-code-login.ts" },
    { "src": "/api/auth/six-digit-code-sign-up", "dest": "/api/auth/six-digit-code-sign-up.ts" },
    { "src": "/api/auth/sign-up", "dest": "/api/auth/sign-up.ts" },
    
    { "src": "/api/leaderboards/flight", "dest": "/api/leaderboards/flight.ts" },
    { "src": "/api/leaderboards/survival", "dest": "/api/leaderboards/survival.ts" }

  ]
}
